<Window x:Name="frmTransport1" x:Class="TuristickaAgencija.Forms.frmTransport"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TuristickaAgencija.Forms"
        mc:Ignorable="d"
        Title="Choose Transportation And Ticket" Height="290" Width="700">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/TuristickaAgencija;component/Graphics/blue_background_picture_2_169441.jpg"/>
        </Grid.Background>
        <Label x:Name="lblTicket" Content="Ticket:" HorizontalAlignment="Left" Margin="54,70,0,0" VerticalAlignment="Top" Width="250"/>
        <ComboBox x:Name="cbxTicket" DisplayMemberPath="Ticket - Seat number" SelectedValuePath="TicketID" HorizontalAlignment="Left" Margin="392,70,0,0" VerticalAlignment="Top" Width="250" Height="26" />
        <Button x:Name="btnSave" Content="Save" HorizontalAlignment="Left" Height="30" Margin="54,195,0,0" VerticalAlignment="Top" Width="170" Click="btnSave_Click"/>
        <Button x:Name="btnCancel" Content="Cancel" HorizontalAlignment="Left" Height="30" Margin="472,195,0,0" VerticalAlignment="Top" Width="170" Click="btnCancel_Click"/>





    </Grid>
</Window>
